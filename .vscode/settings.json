{
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [120],
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.exclude": {
        ".git": true,
        ".vscode/*.log": true,
        "node_modules": true,
        "dist": false
    },
    "eslint.debug": false,
    "eslint.alwaysShowStatus": true,
    "eslint.trace.server": "off"
}
